<div class="container mx-auto mt-5">
  <div class="py-8 m-5">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th scope="col">IdÂ°</th>
          <th scope="col">Nombre</th>
          <th scope="col">Precio</th>
          <th scope="col">Imagen</th>
        </tr>
      </thead>
      <tbody>
        {{#if products}}
          {{#each products }}
          <tr>
            <th>{{this.id}}</th>
            <td>{{this.title}}</td>
            <td>S/.{{this.price}}</td>
            <td><img src="{{this.url}}" alt="{{this.title}}" height="100" width="100"></td>
          </tr>
          {{/each}}
        {{else}}
          <tr>
            <td colspan="4" class="text-center p-20">
              <p class="mb-8">No hay productos para mostrar</p>
            </td>
          </tr>
        {{/if}}
      </tbody>
    </table>
  </div>
</div>